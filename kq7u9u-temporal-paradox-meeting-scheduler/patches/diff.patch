+            # Check constraints
+            violations = self.paradox_detector.validate_schedule_window(
+                current_time, end_time, constraints
+            )
+            
+            if not violations:
+                return current_time
+            
+            # Move to next slot (30-minute increments)
+            current_time += timedelta(minutes=30)
+        
+        return None
