+  const customDaysFormatted = customs.map((c: any) => ({
+    dateISO: DateTime.fromJSDate(c.date, { zone: 'utc' }).setZone(providerTz).toISODate()!,
+    startUtcISO: c.startUtc.toISOString(),
+    endUtcISO: c.endUtc.toISOString(),
+  }))
