+export const handler = async (event: APIGatewayEvent, _context: Context) => {
+  if (event.httpMethod === 'OPTIONS') {
+    return {
+      statusCode: 204,
+      headers: corsHeaders,
+      body: '',
+    }
+  }
+  if (event.httpMethod !== 'POST') {
+    return {
