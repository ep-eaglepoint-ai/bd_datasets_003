+        </div>
+      )}
+    </div>
+  );
+
+  switch (variant) {
+    case 'dropdown':
+      return renderDropdown();
+    case 'buttons':
+      return renderButtons();
+    default:
+      return renderSelect();
+  }
+};
+
+export default TimezoneSelector;
