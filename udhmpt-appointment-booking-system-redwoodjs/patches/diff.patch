+// Reschedule Modal Component
+type RescheduleModalProps = {
+  booking: Booking;
+  customerTz: string;
+  onReschedule: (newStartUtc: string, newEndUtc: string) => void;
+  onClose: () => void;
+  isLoading: boolean;
+  policy: Policy;
+};
