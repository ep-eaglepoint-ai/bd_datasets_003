+import React, { useState } from 'react';
+import { useTimezone } from '../../hooks/useTimezone';
+
+type Props = {
+  value?: string;
+  onChange?: (timezone: string) => void;
+  label?: string;
+  placeholder?: string;
+  disabled?: boolean;
+  showOffset?: boolean;
+  className?: string;
+  variant?: 'select' | 'dropdown' | 'buttons';
+};
+
+export const TimezoneSelector: React.FC<Props> = ({
+  value,
+  onChange,
+  label = 'Timezone',
