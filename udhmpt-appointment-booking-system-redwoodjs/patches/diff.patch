+  const merged = mergeOverrides(expandedWindows, customDaysFormatted)
+
+  const excFormatted = exceptions.map((e: any) => ({
+    startUtcISO: e.startUtc.toISOString(),
+    endUtcISO: e.endUtc.toISOString(),
+  }))
