+        password: hashedPassword,
+      },
+    })
+
+    const token = auth.generateToken(user.id, user.email, user.role)
+    return {
+      statusCode: 201,
+      headers: corsHeaders,
+      body: JSON.stringify({
+        success: true,
+        user: { id: user.id, email: user.email, role: user.role, name: user.name || undefined },
