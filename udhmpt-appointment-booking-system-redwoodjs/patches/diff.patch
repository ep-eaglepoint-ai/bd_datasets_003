+generator client {
+  provider      = "prisma-client-js"
+}
+
+datasource db {
+  provider = "sqlite"
+  url      = env("DATABASE_URL")
+}
+
+model User {
+  id        Int      @id @default(autoincrement())
+  email     String   @unique
+  role      String   @default("CUSTOMER")
+  name      String?
+  password  String?
+  createdAt DateTime @default(now())
+  updatedAt DateTime @updatedAt
+
+  providerProfile ProviderProfile?
+  bookings       Booking[]
+}
+
+model ProviderProfile {
+  id          Int      @id @default(autoincrement())
+  userId      Int      @unique
+  name        String
+  bio         String?
+  timezone    String   @default("UTC")
+  maxBookingsPerDay Int?  // optional cap on total bookings per day (null = unlimited)
+  cancellationFeeCents Int? @default(0)  // penalty for late cancellation
+  rescheduleFeeCents    Int? @default(0)  // penalty for late reschedule
+  penaltiesApplyForLateCancel Boolean @default(false)  // when true, fee applies if cancel/reschedule inside window
+  cancellationWindowHours Int @default(24)  // configurable cutoff window for cancellation
+  rescheduleWindowHours Int @default(24)  // configurable cutoff window for reschedule
+  bookingLeadTimeHours  Int @default(1)   // minimum hours in advance to book
+  createdAt   DateTime @default(now())
+  updatedAt   DateTime @updatedAt
+
+  user               User                     @relation(fields: [userId], references: [id])
+  services           Service[]
+  recurringAvailability RecurringAvailability[]
+  customDayAvailability CustomDayAvailability[]
+  availabilityExceptions AvailabilityException[]
+  manualBlocks       ManualBlock[]
+  bookings           Booking[]
+}
+
+model Service {
+  id                Int      @id @default(autoincrement())
+  providerId        Int
+  name              String
+  durationMinutes   Int
+  capacity          Int      @default(1)
+  bufferBeforeMinutes Int     @default(0)
+  bufferAfterMinutes  Int     @default(0)
+  createdAt         DateTime @default(now())
+  updatedAt         DateTime @updatedAt
+
+  provider ProviderProfile @relation(fields: [providerId], references: [id])
+  bookings Booking[]
+}
+
+model RecurringAvailability {
+  id           Int      @id @default(autoincrement())
+  providerId   Int
+  weekday      Int      // 1 = Monday .. 7 = Sunday (Luxon convention)
+  startLocal   String   // "09:00" in provider TZ
+  endLocal     String   // "17:00"
+  tz           String   @default("UTC")
+  createdAt    DateTime @default(now())
+  updatedAt    DateTime @updatedAt
+
+  provider ProviderProfile @relation(fields: [providerId], references: [id])
+}
+
+model CustomDayAvailability {
+  id           Int      @id @default(autoincrement())
+  providerId   Int
+  date         DateTime
+  startUtc     DateTime
+  endUtc       DateTime
+  tz           String   @default("UTC")
+  createdAt    DateTime @default(now())
+  updatedAt    DateTime @updatedAt
+
+  provider ProviderProfile @relation(fields: [providerId], references: [id])
+}
+
+model AvailabilityException {
+  id         Int      @id @default(autoincrement())
+  providerId Int
+  startUtc   DateTime
+  endUtc     DateTime
+  reason     String?
+  createdAt  DateTime @default(now())
+  updatedAt  DateTime @updatedAt
+
+  provider ProviderProfile @relation(fields: [providerId], references: [id])
+}
+
+model ManualBlock {
+  id         Int      @id @default(autoincrement())
+  providerId Int
+  startUtc   DateTime
+  endUtc     DateTime
+  reason     String?
+  createdAt  DateTime @default(now())
+  updatedAt  DateTime @updatedAt
+
+  provider ProviderProfile @relation(fields: [providerId], references: [id])
+}
+
+model Booking {
+  id           Int       @id @default(autoincrement())
+  version      Int       @default(1)  // optimistic locking
+  providerId   Int
+  serviceId    Int
+  startUtc     DateTime
+  endUtc       DateTime
+  customerEmail String
+  userId       Int
+  capacitySlot Int?       @default(0) // used for DB-level concurrency safety
+  reference    String    @unique
+  canceledAt   DateTime?
+  status       String    @default("pending")
+  notes        String?
+  penaltyFeeCents Int?   @default(0)
+  createdAt    DateTime  @default(now())
+  updatedAt    DateTime  @updatedAt
+
+  provider ProviderProfile @relation(fields: [providerId], references: [id])
+  service  Service          @relation(fields: [serviceId], references: [id])
+  user     User             @relation(fields: [userId], references: [id])
+
+  @@unique([providerId, startUtc, capacitySlot])
+}
